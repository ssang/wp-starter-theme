{
  "name": "crew-starter",
  "private": true,
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && bun run blocks:build",
    "blocks": "wp-scripts start --webpack-src-dir=./resources/blocks/ --output-path=./dist/blocks",
    "blocks:build": "wp-scripts build --webpack-src-dir=./resources/blocks/ --output-path=./dist/blocks"
  },
  "devDependencies": {
    "@kucrut/vite-for-wp": "^0.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@wordpress/block-editor": "^13.1.0",
    "@wordpress/create-block": "^4.34.0",
    "@wordpress/i18n": "^5.1.0",
    "@wordpress/scripts": "^27.6.0",
    "postcss-easy-import": "^4.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@wordpress/browserslist-config": "^5.7.0",
    "alpinejs": "^3.13.5",
    "autoprefixer": "latest",
    "gsap": "^3.12.5",
    "postcss": "latest",
    "rollup-plugin-external-globals": "^0.10.0",
    "swiper": "^11.1.0",
    "tailwindcss": "^3.4.1",
    "vite-plugin-external": "^4.3.1"
  }
}