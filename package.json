{
  "name": "crew-starter",
  "private": true,
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && bun run blocks:build",
    "blocks": "wp-scripts start --webpack-src-dir=./resources/blocks/ --output-path=./dist/blocks",
    "blocks:build": "wp-scripts build --webpack-src-dir=./resources/blocks/ --output-path=./dist/blocks"
  },
  "devDependencies": {
    "@wordpress/create-block": "^4.14.0",
    "@wordpress/scripts": "^26.1.0",
    "@kucrut/vite-for-wp": "^0.5.2",
    "postcss-easy-import": "^4.0.0",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@fontsource/cairo": "^4.5.13",
    "@fontsource/poppins": "^4.5.10",
    "@wordpress/browserslist-config": "^5.7.0",
    "alpinejs": "latest",
    "autoprefixer": "latest",
    "postcss": "latest",
    "tailwindcss": "latest"
  }
}